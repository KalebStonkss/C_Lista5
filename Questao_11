#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define TAM 10
int main() {
    int max;
    int *mx = &max;
    int *x = (int *)malloc(TAM * sizeof(int));
    int *y = (int *)malloc(TAM * sizeof(int));
    int **m = (int **)malloc(TAM * sizeof(int *));
    for(int i=0;i<TAM;i++){
        m[i] = (int *)calloc(TAM,sizeof(int));
    }
    puts("Digite o valor mÃ¡ximo do intervalo: \n");
    scanf("%d",mx);
    srand(time(NULL));
    for(int i = 0;i<TAM;i++){
        x[i] = rand() % *mx;
        y[i] = rand() % *mx;
    }
    printf("\n");
    printf("Vetor Y: \n");
    for(int i =0;i<TAM;i++){
        printf("|%d| ",x[i]);
    }
    printf("\nVetor X: \n");
    for(int i =0;i<TAM;i++){
        printf("|%d| ",y[i]);
    }
    printf("\n");
    printf("Matriz total preenchida com as contagens");
    printf("\n");
    for(int i=0;i<TAM;i++){
        if(x[i] < TAM && y[i] < TAM){
            m[x[i]][y[i]]++;
        }
    }
    for(int i=0;i<TAM;i++){
        for(int j=0;j<TAM;j++){
            printf("|%d|\t",m[i][j]);
        }
        printf("\n");
    }
    free(x);
    free(y);
    for(int i =0;i<TAM;i++){
        free(m[i]);
    }
    free(m);
    return 0;
}
