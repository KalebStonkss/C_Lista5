#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define TAM 14
int main() {
    int max;
    int *mx = &max;
    puts("Digite o valor m√°ximo do intervalo: ");
    scanf("%d",mx);
    int *x = (int *)malloc(TAM * sizeof(int));
    int *f = (int *)malloc(*mx * sizeof(int));
    srand(time(NULL));
    for(int i = 0;i<*mx;i++){
        f[i] = 0;
    }
    for(int i = 0; i<TAM;i++){
        x[i] = rand() % *mx;
    }
    for(int i = 0; i<TAM;i++){
        printf("|%d|",x[i]);
        printf("\n");
    }
    for(int i = 0;i<TAM;i++){
        for(int j = 0;j<TAM;j++){
            if(x[i] == j){
                f[j]++;
            }
        }
    }
    printf("Vetor F \n");
    for(int i = 0;i<*mx;i++){
        printf("Contagem em %d = %d \n",i,f[i]);
    }
    return 0;
}
